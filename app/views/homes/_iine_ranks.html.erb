<div>
  <%= @iine_ranks.each do |game, i| %>
  <table class="table">
    <tbody>
      <tr>
        <th>第<%= i %>位</th>
      </tr>
      <tr>
        <th>
          <%= game.favorites.count %>
        </th>
          
        <th>
          <%= link_to public_game_path(game), method: :delete do %>
            <%= game.game_title %>
          <% end %>
        </th>
      </tr>
    </tbody>
  </table>
  <% end %>
</div>